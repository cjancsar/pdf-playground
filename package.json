{
  "name": "pdf-playground",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npx parcel ./src/form.html ./src/pdfs/fw4.pdf --port 3000",
    "prettify": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "test": "jest --detectOpenHandles --forceExit --verbose"
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.6",
    "@types/file-saver": "^2.0.1",
    "@types/jest": "^26.0.3",
    "@types/lodash": "^4.14.157",
    "@types/pdfjs-dist": "^2.1.4",
    "jest": "^26.1.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "ts-loader": "^7.0.5",
    "tsc-watch": "^4.2.9",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-jest": "^1.0.5",
    "tslint-jsdoc-rules": "^0.2.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "envinfo": "^7.5.1",
    "express": "^4.17.1",
    "file-saver": "^2.0.2",
    "lodash": "^4.17.19",
    "pdf-lib": "^1.9.0",
    "pdfjs-dist": "2.5.207",
    "rimraf": "^3.0.2",
    "src": "^1.1.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
